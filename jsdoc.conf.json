{
  "source": {
      "include": ["awsConfig", "controllers", "models"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
  }
}
